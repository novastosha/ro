<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <!-- Appenders -->
    <Appenders>
        <!-- Console Appender -->
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="[%c] [%t] [%d{HH:mm:ss}]: %msg%n" />
        </Console>
    </Appenders>

    <!-- Loggers -->
    <Loggers>
        <!-- Root Logger -->
        <Root level="debug">
            <AppenderRef ref="Console" />
        </Root>
    </Loggers>
</Configuration>
